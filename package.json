    "@storybook/addon-viewport": "^4.1.2",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/json-schema": "^7.0.1",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.2",
