{
  "name": "@retailmenot/anchor",
  "version": "0.0.1-alpha.3",
  "description": "A React UI Library by RetailMeNot",
  "main": "commonjs/index.js",
  "module": "esm/index.es.js",
  "repository": "retailmenot/anchor.git",
  "bugs": {
    "url": "https://github.com/retailmenot/anchor"
  },
  "homepage": "https://github.com/retailmenot/anchor#readme",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "commonjs",
    "esm"
  ],
  "keywords": [
    "RetailMeNot",
    "UI",
    "React",
    "Javascript",
    "styled-components",
    "ESNext",
    "NextJS",
    "Isomorphic",
    "Component",
    "Library",
    "Gatsby"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "clean": "rm -rf esm commonjs ts .cache && git clean -Xdf",
    "build": "concurrently --names 'modules,typescript' 'npm run build:modules' 'npm run build:ts'",
    "build:ts": "tsc",
    "build:modules": "rollup -c rollup.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -s .storybook/static -o .out",
    "prepublishOnly": "npm run build",
    "lint": "concurrently --names 'prettier,tslint' 'npm run pretty' 'npm run tslint'",
    "pretty": "prettier --config .prettierrc --write \"./src/**/*.{tsx,ts}\"",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "static": ""
  },
  "author": "Kameron (@sharkparty)",
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": ">= 3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
