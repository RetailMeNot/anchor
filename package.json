    "yargs": "^14.0.0"
  },
  "dependencies": {
    "@xstyled/styled-components": "^1.16.0",
    "@xstyled/system": "^1.16.0",
    "classnames": "^2.2.6",
    "core-js": "^3.0.0",
    "polished": "^3.2.0",
    "react-responsive": "^8.0.1",
    "rxjs": "^6.5.2",
    "styled-react-modal": "^1.2.1",
    "ts-debounce": "^1.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "sideEffects": false
}
