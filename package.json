  "dependencies": {
    "@rebass/grid": "^6.0.0-7",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "polished": "^2.3.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "rxjs": "^6.3.3"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "sideEffects": false
